pipeline {
    agent any

    stages {
        stage('deploy to dev') {
            steps {
                echo 'Hello World out'
            }
        }
    
    
        stage('test to qa') {
            steps {
                echo 'Hello World in'
            }
        }
    

        stage('deploy to prod') {
            steps {
                echo 'Hello deploy to prod'
            }
        }
    }

    

}
