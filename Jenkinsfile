pipeline {
    agent any

    stages {
        stage('build-dev') {
            steps {
                echo 'Hello World out'
            }
        }
    }
    stages {
        stage('test now') {
            steps {
                echo 'Hello World in'
            }
        }
    }
    stages {
        stage('deploy') {
            steps {
                echo 'Hello World here'
            }
        }
    }

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World over'
            }
        }
    }



}
